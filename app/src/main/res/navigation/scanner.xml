<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scanner"
    app:startDestination="@id/scannerFragment">

    <fragment
        android:id="@+id/scannerFragment"
        android:name="ru.codeoverflow.openspaceapp.ui.fragment.scanner.ScannerFragment">
        <argument
            android:name="addressId"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="meter"
            android:defaultValue="@null"
            app:argType="ru.codeoverflow.openspaceapp.entity.core.detailaddress.MeterModel"
            app:nullable="true" />
    </fragment>
</navigation>